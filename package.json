    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "webpack": "4.41.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.2.1"
  }
